{
  "name": "wspa",
  "version": "0.4.0",
  "description": "CLI tool for creating workspaces",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "@wspa/cli": "workspace:*"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "engine": {
    "node": ">=16.0.0"
  },
  "x-wspa": {
    "pkgManifest": {
      "bin": "index.js"
    }
  }
}
